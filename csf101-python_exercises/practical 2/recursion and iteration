# IMPLEMENTING RECURSION 
def fibonacci (n):
    if n == 0:
        return 0
    elif n == 1:
        return 1
# recursive case: Fn = Fn-1 + Fn-2
    return fibonacci (n-1) + fibonacci (n-2) 
n = 10
print(f"Fibonacci({n}) = {fibonacci(n)}")
# IMPLEMENTING ITERATION
def fibonacci (n):
    if n == 0:
        return 0
    elif n == 1:
        return 1
    a, b = 0, 1
    for _ in range(2, n+1):
     a, b = b, a + b
    return b

n = 10
print (f"Fibonacci({n}) = {fibonacci(n)}")